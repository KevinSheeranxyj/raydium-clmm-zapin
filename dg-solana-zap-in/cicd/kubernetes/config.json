{
  "isInit": true,
  "workloadResource": "deployment",
  "replicas": 1,
  "serviceClusterIP": "None",
  "deploymentStrategy": {
    "type": "RollingUpdate",
    "rollingUpdate": {
      "maxSurge": "25%",
      "maxUnavailable": "25%"
    }
  },
  "dev": {
    "namespace": "dev-backend",
    "consulPrefix": "DEV",
    "efsHost": "/efs",
    "efsEnvPrefix": "/dev"
  },
  "testd": {
    "namespace": "testd-be",
    "consulPrefix": "testd",
    "efsHost": "/efs",
    "efsEnvPrefix": "/testd"
  },
  "teste": {
    "namespace": "teste-be",
    "consulPrefix": "teste",
    "efsHost": "/efs",
    "efsEnvPrefix": "/teste"
  },
  "testf": {
    "namespace": "testf-be",
    "consulPrefix": "testf",
    "efsHost": "/efs",
    "efsEnvPrefix": "/testf"
  },
  "staging": {
    "namespace": "backend-be",
    "consulPrefix": "STG",
    "efsHost": "/efs",
    "efsEnvPrefix": "/backend"
  },
  "production": {
    "namespace": "backend-be",
    "consulPrefix": "PRD",
    "efsHost": "/efs",
    "efsEnvPrefix": "/backend"
  }
}
